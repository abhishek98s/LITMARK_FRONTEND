{
  "name": "litmark",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check --open",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run Litmark:serve-ssr",
    "serve:ssr": "node dist/Litmark/server/main.js",
    "build:ssr": "ng build && ng run Litmark:server",
    "prerender": "ng run Litmark:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^18.0.0",
